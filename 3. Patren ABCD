#include<iostream>
using namespace std;
 
 int main()
 {
    int n = 10;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n-i;j++)
        {
            cout << char(j+65);
        }
        
        for(int l=0;l<((2*i)-1);l++)
        {
            cout << " ";
        }

        for(int k=n-i-1;k>=0;k--)
        {if(n-1 == k){continue;}
        else{
            cout << char(k+65);
        }
        }
        cout << endl;
        
    }
 }
