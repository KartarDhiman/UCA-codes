#include<iostream>
using namespace std;

bool check(int a, int size, int *arr)
{
    for(int i=0;i<size;i++)
    {
        if(arr[i]==a)
        {return 0;}
        else{continue;}
        return 1;
    }
}

int main()
{
    int size,*arr,a;
    cin >> size;
    arr = new int[size];

    for(int i=0;i<size;i++)
    {
        cout << "Enter Number: ";
        cin >> a;
        if(check(a,size,arr)
        {
            arr[i] = a;
        }
        else
        {
            cout << "This Value Already Exists.";
            i--;
        }
    }
    for(int i=0;i<size;i++)
    {
        cout << arr[i] << " ";
    }
}
